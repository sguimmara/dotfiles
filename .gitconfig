[color]
	ui = auto
[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
